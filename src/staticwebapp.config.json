{
  "routes": [
    {
        "route": "/*",
        "allowedRoles": [
            "authenticated"
        ]
    }
  ],
  "responseOverrides": {
    "401": {
        "redirect": "/.auth/login/aad",
        "statusCode": 302
    }
  },
  "auth": {
    "identityProviders": {
      "azureActiveDirectory": {
        "registration": {
          "openIdIssuer": "TENANT_ID",
          "clientIdSettingName": "AZURE_CLIENT_ID",
          "clientSecretSettingName": "AZURE_CLIENT_SECRET"
        }
      }
    }
  }
}
